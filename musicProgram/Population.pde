void population () {
  //Main Box
  boxX = width*0 ;
  boxY = height*0 ;
  boxWidth = width*1/2 ;
  boxHeight = height*1/2 ;
  
  playPauseX = width*1/2;
  playPauseY = height*1/2;
  playPauseDiameter = 1/4;
  
  stopX = width*1/3 ;
  stopY = height*1/2 ;
  stopDiameter = 1/4 ;
  
  forwardX = width ;
  forwardY = height ;
  forwardWidth = width ;
  forwardHeight = height ;
  
  reverseX = width;
  reverseY = height ;
  reverseWidth = width ;
  reverseHeight = height ;
}
